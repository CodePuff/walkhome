<!DOCTYPE html>
<html>
  <%-include('../partials/head') %>
  <body>
    <%- include('../partials/header') %>
      <ul>
        <li id="localStorageEmail" class="hide-me"></li>
        <li>WalkScore: <%= search[5] %> </li>
        <li>WalkScore Explanation: <%= search[6] %> </li>
        <li><strong><a href= <%= search[7] %>>WalkScore Details</a></strong></li>
        <% if(search[0]) { %>
        <li>Address: <%= search[0] %></li>
        <% } %>
        <li>City: <%= search[2] %></li>
        <% if(search[1]) { %>
          <li>Zip: <%= search[1] %></li>
          <% } %>
      </ul>
      <p>
        <strong><em><%= message %></em></strong>
      </p>
      <p>
        <a id='return-to-address-search' href='/address'>Return to Address Search</a>
      </p>
    <%- include('../partials/footer') %>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
    <script>
      $(function() {
        const myEmail = localStorage.getItem('user_email');
        $('#localStorageEmail').text(myEmail);
        console.log('myEmail', myEmail);
        console.log($('#localStorageEmail'));
        console.log('localStorageEmail dot text()', $('#localStorageEmail').text());
      });
    </script>
  </body>
</html>